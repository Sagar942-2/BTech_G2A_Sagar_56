#include<stdio.h>
#include<math.h>
#define pi 3.14
int main()
{
    float area,circumference,radius;
    printf("enter the value of radius:");
    scanf("%f",&radius);
    area=pi*radius*radius;
    circumference=2*pi*radius;
    printf("area of circle: %f\n",area);
    printf("circumference:%f\n",circumference);
    return 0;


}

